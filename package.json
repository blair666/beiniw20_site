{
  "name": "beiniw20-portfolio",
  "version": "1.0.0",
  "description": "Personal portfolio website for beiniw20.com showcasing full-stack, SRE, and AI skills",
  "main": "index.js",
  "scripts": {
    "dev": "live-server --port=3000",
    "build": "npm run build:css",
